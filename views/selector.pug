extends layout

block content
  h1= title
  p Welcome to #{title} Js
  br
  - var image_names= ["fall.jpeg","hallowen.jpeg","mario.jpeg","apple.jpeg","snow.jpeg"]            
    -for(let i=0;i<3;i++){
        - let randnum=Math.floor(Math.random()*5)
            img(src= `/images/${image_names[randnum]}` alt="image", height="300", width="300")
        -}